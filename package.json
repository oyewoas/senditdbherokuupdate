{
  "name": "senditdb",
  "version": "1.0.0",
  "description": "A parcel delivery application database Api",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register \"./tests/*.spec.js\" --timeout 150000",
    "create-tables": "babel-node ./app/db/db createAllTables",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "node -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && NODE_ENV=production node ./build/server.js",
    "coverage": "babel-node ./node_modules/.bin/babel-istanbul  cover _mocha -- -R spec \"./test/*.spec.js\" --timeout 15000000 --compilers js:babel-core/register",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "dev-start": "nodemon --watch . --exec babel-node -- server.js --presetenv",
    "builded": "npm run pre-start && npm run create-tables && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyewoas/senditdatabase.git"
  },
  "keywords": [],
  "author": "Ayooluwa Oyewo",
  "license": "ISC",
  "engines": {
    "node": "10.15.0"
  },
  "bugs": {
    "url": "https://github.com/oyewoas/senditdatabase/issues"
  },
  "homepage": "https://github.com/oyewoas/senditdatabase#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.23.0",
    "path": "^0.12.7",
    "pg": "^7.8.0",
    "request": "^2.88.0",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.8",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10"
  }
 
}
